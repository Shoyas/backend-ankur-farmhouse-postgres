{
    "version": 2,
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      },
      { "src": "package.json", "use": "@vercel/node" },
      { "src": "vercel-build.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/server.js"
      },
      { "src": "/.*", "dest": "vercel-build.js" }
    ]
  }

  
  